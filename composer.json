{
    "name": "bear/dev-package",
    "description": "dev-require packages for BEAR.Package",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bear/ace": "1.*",
        "classpreloader/classpreloader": "dev-wrap-namespace",
        "dg/adminer-custom": "1.*",
        "facebook/xhprof": "dev-master",
        "firephp/firephp-core": "*",
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.3.*",
        "printo/printo": "0.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-stdlib": "2.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type": "package",
            "package": {
                "name": "classpreloader/classpreloader",
                "version": "dev-wrap-namespace",
                "dist": {
                    "url": "https://github.com/koriym/classpreloader/zipball/wrap-namespace",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/koriym/ClassPreloader.git",
                    "type": "git",
                    "reference": "wrap-namespace"
                },
                "require": {
                    "symfony/console": ">2.0",
                    "symfony/console": ">2.0",
                    "symfony/filesystem": ">2.0",
                    "symfony/finder": ">2.0",
                    "nikic/php-parser": "*"
                }
            }
        }
    ]
}
